{
  "name": "docs-example",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/docs-example",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "command": "gatsby build --verbose",
        "outputPath": "dist/apps/docs-example",
        "color": true
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "docs-example:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "docs-example:build:development"
        },
        "production": {
          "buildTarget": "docs-example:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/docs-example/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/docs-example/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "implicitDependencies": ["gatsby-theme-joy-ui"],
  "tags": []
}
